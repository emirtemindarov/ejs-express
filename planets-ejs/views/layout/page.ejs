<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
  <link href="/bootstrap/dist/css/bootstrap.css" rel="stylesheet" type="text/css">
  <script src="/jquery/dist/jquery.js"></script>
  <script src="/bootstrap/dist/js/bootstrap.js"></script>
</head>
<body>
  <nav class="dss navbar navbar-expand-lg navbar-light bg-light">
    <div class="container-fluid">
      <a class="navbar-brand pad" href="/">Все все все ...</a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Переключатель навигации">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNav">
        <% if(typeof nav == 'object' && nav){
          nav.forEach(function(item){
        %>
          <!-- <li class="nav-item"> -->
            <a class="nav-item nav-link" href="/planets/<%=item.nick%>"><%=item.title%></a>
          <!-- </li> -->
        <%
        })}
        %>
        <ul class="nav navbar-nav ms-auto">
          <li class="nav-item">
            <a class="nav-link" aria-current="page" href="#">Войти</a>
          </li>
        </ul>
      </div>
    </div>
  </nav>
  <%- body %>
</body>
</html>
